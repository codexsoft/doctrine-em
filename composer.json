{
    "name": "codexsoft/doctrine-em",
    "license": "MIT",
    "description": "Doctrine Entity Manager wrappers for various purposes",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8",
        "doctrine/orm": "^2.6",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "CodexSoft\\DoctrineEm\\": "src/",
            "": ["vendor/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": ["tests/unit/"]
        }
    }
}
